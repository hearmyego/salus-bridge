{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "release-type": "python",
  "packages": {
    ".": {
      "extra-files": [
        {
          "type": "generic",
          "path": "app/__version__.py",
          "glob": false,
          "content": "__version__ = \"${version}\"\n"
        }
      ]
    }
  }
}
